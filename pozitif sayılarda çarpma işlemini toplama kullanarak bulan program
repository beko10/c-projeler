#include <iostream>
 
using namespace std;

int main ( ) {
	
	//pozitif sayılarda çarpma işlemini toplama kullanarak bulan program
	int sayi1,sayi2;
	cout<<"lutfen sayi1 degerini giriniz";cin>>sayi1;
	cout<<"lutfen sayi2 degerini giriniz";cin>>sayi2;
	int carpmasonucu=0;
	for(int i=0;i<sayi1;i++)
	{
		carpmasonucu+=sayi2;
	}
	cout<<carpmasonucu;
	
	return 0;
}
