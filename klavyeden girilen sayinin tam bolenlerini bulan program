#include <iostream>

using namespace std;

// klavyeden girilen sayinin tam bolenlerini bulan program
int main( ) {
	int sayi;
	cout<<"lutfen bir sayi giriniz";cin>>sayi;
	cout<<sayi<<"'in tam bolenleri=";
	for(int i=1;i<=sayi;i++)
	{
		if(sayi%i==0)
		{
			
			cout<<i<<",";
			
		}
	}
return 0;	
}
