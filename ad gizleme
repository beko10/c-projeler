#include <iostream>

using namespace std;

//ad gizleme

int main() {
    
    string isim;
    cin>>isim;
    int x=isim.length();
		
		cout<<isim[0];
		for(int i=1;i<x;i++)
		cout<<" * ";
	return 0;
}
